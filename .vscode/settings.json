{
    "python.pythonPath": "${workspaceFolder}/venv/bin/python",
    "python.venvPath": "{workspaceFolder}/venv/bin/python",
    "python.linting.pylamaPath": "{workspaceFolder}/venv/lib/python3.6/site-packages/pylama",
    "python.linting.pylamaEnabled": false,
    "python.unitTest.nosetestsEnabled": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true,
        "venv": true,
        ".noseids": true,
        "**/*.db":true,
        "**/*.pyc":true
    }
}